{
  "name": "Impressions",
  "version": "0.0.1",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Edwin Uber": "edwin@getepic.com"
  },
  "homepage": "https://getepic.com/",
  "summary": "ImpressionsGRPC",
  "source": {
    "git": "https://github.com/grpc/grpc-swift.git"
  },
  "platforms": {
    "ios": "9.0",
    "osx": "10.9"
  },
  "prepare_command": "mkdir -p Pods/Impressions\nprotoc     gRPC/*.proto     --swift_out=.     --swiftgrpc_out=Client=true,Server=false:.",
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "subspecs": [
    {
      "name": "Messages",
      "source_files": "gRPC/*.pb.swift",
      "header_mappings_dir": "Pods/Impressions",
      "requires_arc": false,
      "dependencies": {
        "SwiftProtobuf": [

        ]
      }
    },
    {
      "name": "Services",
      "source_files": "gRPC/*.grpc.swift",
      "header_mappings_dir": "Pods/Impressions",
      "requires_arc": true,
      "dependencies": {
        "SwiftGRPC": [

        ],
        "Impressions/Messages": [

        ]
      }
    }
  ]
}
